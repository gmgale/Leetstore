{
  "name": "nodeapp1",
  "version": "1.0.0",
  "description": "A simple node app.",
  "main": "app.js",
  "engines": {
    "node": ">12.22.9"
  },
  "scripts": {
    "test": "NODE_ENV=test jest --detectOpenHandles --bail",
    "test-serial": "NODE_ENV=test jest --detectOpenHandles --runInBand",
    "start:dev": "NODE_ENV=development node --trace-warnings dist/server.js",
    "start:ts": "NODE_ENV=development node -r ts-node/register src/server.ts",
    "start:prod": "NODE_ENV=production nodemon dist/server.js",
    "debug": "ndb server.js",
    "build": "rm -rf dist && tsc -p tsconfig-build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gmgale/NodeApp1.git"
  },
  "author": "George Gale",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gmgale/NodeApp1/issues"
  },
  "homepage": "https://github.com/gmgale/NodeApp1#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "mongodb-memory-server": "^8.10.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.6.0",
